{
  "name": "nutria-macro",
  "version": "1.0.0",
  "description": "Modern nutrition planning platform",
  "type": "module",
  "main": "index.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "python -m http.server 8000",
    "api": "node server/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings=0",
    "test": "vitest --run",
    "test:api": "cross-env API_PORT=3001 node server/index.js & wait-on http://localhost:3001/health && vitest --run tests/api.search.test.ts && kill-port 3001",
    "feed": "node toolsupdate-agent-feed.js",
    "feed:auto": "node toolsupdate-agent-feed.js --summary \"$(git log -1 --pretty=%s)\""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/supabase-js": "^2.57.2",
    "dotenv": "^17.2.2",
    "zod": "^3.23.8",
    "lucide-react": "^0.543.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "kill-port": "^2.0.1",
    "wait-on": "^7.2.0",
    "@eslint/js": "^9.35.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.0.0",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "nutrition",
    "macro",
    "health",
    "react",
    "typescript"
  ],
  "author": "NUTRIA Team",
  "license": "MIT"
}
